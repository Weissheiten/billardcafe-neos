{
    "name": "neos/neos-base-distribution",
    "description": "Neos Base Distribution",
    "license": "GPL-3.0+",
    "support": {
        "email": "hello@neos.io",
        "slack": "http://slack.neos.io/",
        "forum": "https://discuss.neos.io/",
        "wiki": "https://discuss.neos.io/c/the-neos-project/project-documentation",
        "issues": "https://github.com/neos/neos-development-collection/issues",
        "docs": "http://neos.readthedocs.org/",
        "source": "https://github.com/neos/neos-base-distribution"
    },
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin"
    },
    "repositories": {
        "0": {
            "type": "vcs",
            "url": "https://Weissheiten@bitbucket.org/Weissheiten/billardcafe.home.git"
        },
        "1": {
            "type": "vcs",
            "url": "https://github.com/Weissheiten/Weissheiten.Neos.News.git"
        },
        "2": {
            "type": "vcs",
            "url": "https://github.com/Weissheiten/Weissheiten.Neos.InstagramMedia.git"
        },
        "3": {
            "type": "vcs",
            "url": "https://github.com/Weissheiten/Weissheiten.OAuth2.ClientInstagram.git"
        },
        "4": {
            "type": "vcs",
            "url": "https://github.com/Weissheiten/Flowpack.OAuth2.Client.git"
        },
        "5": {
            "type": "vcs",
            "url": "https://github.com/Weissheiten/Weissheiten.Flow.WiFiGuestCredentialsProvider.git"
        },
        "weissheitenmediatools": {
            "type": "vcs",
            "url": "https://github.com/Weissheiten/Weissheiten.Neos.MediaTools"
        },
        "weissheitenfacebooksdk": {
            "type": "vcs",
            "url": "https://github.com/Weissheiten/neos-facebookgraphsdk"
        }
    },
    "require": {
        "neos/neos": "~3.3.0",
        "neos/nodetypes": "~3.3.0",
        "neos/site-kickstarter": "~3.3.0",
        "neos/seo": "~2.0.0",
        "neos/setup": "~4.0.0",
        "flowpack/neos-frontendlogin": "~3.0",
        "neos/redirecthandler-neosadapter": "~2.0",
        "neos/redirecthandler-databasestorage": "~2.0",
        "moc/notfound": "~3.0",
        "flowpack/oauth2-client": "^1.0",
        "weissheiten/oauth2-clientinstagram": "^1.0",
        "weissheiten/neos-bootstrap": "~2.1.2",
        "weissheiten/neos-news": "~2.2.2",
        "weissheiten/billardcafe-home": "^3.8.3",
        "weissheiten/neos-instagrammedia": "~1.1",
        "weissheiten/neos-nodemigration": "^1.0",
        "weissheiten/neos-facebookgraphsdk": "dev-master",
        "neos/swiftmailer": "~6.0.0",
        "weissheiten/flow-wifiguestcredentialsprovider": "^1.0.0",
        "moc/imageoptimizer": "^3.0",
        "ttree/stylesheet": "^3.0",
        "ttree/script": "^3.0",
        "neos/neos-ui": "~1.0",
        "neos/neos-ui-compiled": "~1.0",
        "weissheiten/neos-mediatools": "dev-master",
        "facebook/graph-sdk": "^5.7",
        "jonnitto/googlemaps": "~3.0"
    },
    "replace": {
        "typo3/neos-base-distribution": "self.version"
    },
    "scripts": {
        "post-update-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "neos/buildessentials": "^5.2",
        "mikey179/vfsStream": "^1.6"
